    "### 시간대 설정하기 "
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "source": [
    "from datetime import datetime\r\n",
    "from pytz import timezone\r\n",
    "\r\n",
    "kst = timezone('Asia/Seoul')\r\n",
    "cur_time = datetime.now(kst)\r\n",
    "print(cur_time)\r\n",
    "ts = int(datetime.now().timestamp())\r\n",
    "time_now = datetime.fromtimestamp(ts,kst).strftime(\"%y/%m/%d %H:%M:%S\")\r\n",
    "\r\n",
    "print(time_now)"
   ],
